<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="file_description_frame">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Button" id="file_description_back">
        <property name="text" translatable="yes">Back</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="file_description_frame_label">
        <property name="text" translatable="yes">Enter any description you may have for this file:</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="file_description_text">
        <property name="height">10</property>
        <property name="width">50</property>
        <layout manager="pack">
          <property name="padx">10</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="file_description_edit_button">
        <property name="text" translatable="yes">Edit</property>
        <layout manager="pack">
          <property name="propagate">True</property>
          <property name="side">bottom</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
