<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="edit_option_frame">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Button" id="edit_option_back">
        <property name="text" translatable="yes">Back</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Separator" id="edit_option_sparator_1">
        <property name="orient">horizontal</property>
        <layout manager="pack">
          <property name="fill">x</property>
          <property name="pady">10 0</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="edit_option_name_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="padx">10</property>
          <property name="pady">10 0</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Label" id="edit_option_name_label">
            <property name="text" translatable="yes">Name: </property>
            <layout manager="pack">
              <property name="propagate">True</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="edit_option_name_entry">
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">x</property>
              <property name="propagate">True</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="edit_option_default_value_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="padx">10</property>
          <property name="pady">10 0</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Label" id="edit_option_default_value_label">
            <property name="text" translatable="yes">Default Value: </property>
            <layout manager="pack">
              <property name="anchor">w</property>
              <property name="propagate">True</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="edit_option_default_value_entry">
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">x</property>
              <property name="propagate">True</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="edit_option_comment_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="padx">10</property>
          <property name="pady">10 0</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Label" id="edit_option_comment_label">
            <property name="text" translatable="yes">Comment: </property>
            <layout manager="pack">
              <property name="anchor">w</property>
              <property name="propagate">True</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="edit_option_comment_entry">
            <property name="width">50</property>
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">x</property>
              <property name="propagate">True</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="edit_option_save_button">
        <property name="text" translatable="yes">Save</property>
        <layout manager="pack">
          <property name="pady">10 0</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Separator" id="edit_option_sparator_2">
        <property name="orient">horizontal</property>
        <layout manager="pack">
          <property name="fill">x</property>
          <property name="pady">10 0</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="edit_option_possible_values_button">
        <property name="text" translatable="yes">Manage Possible Values</property>
        <layout manager="pack">
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
